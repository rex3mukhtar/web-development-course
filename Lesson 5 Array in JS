let friends = ["Mukku","Ijaz","Atif"]
let fruits = ["Pineapple","Strawberry","Plum"]
let numbers = [1,2,3]

console.log(friends)
console.log(fruits)
console.log(numbers)

//add
fruits.push("Dragon fruit")
fruits.push("Star fruit")
console.log(fruits)

//remove
numbers.pop()
numbers.pop()
console.log(numbers)

//slice
console.log(friends.slice(0, 2))
console.log(numbers.slice(1,3))

//indexing
console.log(fruits[1])
console.log(friends[0])

for (let name of fruits){
    console.log(name)
}

for (let name in fruits){
    console.log(name)
}

for (let name in fruits){
    console.log(fruits[name])
}
