school = "codingal"
for i in school:
    print(i)

for i in range(5):
    print(i,"Hello")

for i in range(1,10):
    print(i,"bye")

for i in range(10,26):
    if i%2!=0:
        print(i)

for i in range(10,26):
    if i%2==0:
        print(i)

sum = 0
for i in range(1,11):
    sum = sum + i
    print(sum)
print(sum)

fruits = ["apple","cherry","banana","grapes"]
for i in fruits:
    if i=="banana":
        print("True")
