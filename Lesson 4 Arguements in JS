<!DOCTYPE html>
<html>
    <body>
        <h1>Largest Number</h1>
        <p id="result"></p>
        <script>
            function max(){
                var max=0;
                for(i=0; i<arguments.length; i++){
                    if (arguments[i]>max){
                        max = arguments[i];
                    }
                }
                return max;
            }
            document.getElementById("result").innerHTML=max(10383,7483,98274374,36373);
        </script>
    </body>
</html>
