age=32
if age>18:
    print("Eligible")
else:
    print("Not Eligible")

day=3
if day==1:
    print("Sunday")
elif day==2:
    print("Monday")
elif day==3:
    print("Tuesday")
else:
    print("Invalid!")

x=5
result = "Even" if x%2==0 else "Odd"
print(result)
polarity = "Positive" if x>0 else "Negative"
print(polarity)

print(True and False)
print(True and True)
print(False and True)
print(False and False)

print(True or False)
print(True or True)
print(False or True)
print(False or False)

print(not True)
print(not False)
